    "mongodb": "^3.6.7",
    "mongodb-client-encryption": "^1.2.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.20.1",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@types/mongodb": "^3.6.13",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.15",
